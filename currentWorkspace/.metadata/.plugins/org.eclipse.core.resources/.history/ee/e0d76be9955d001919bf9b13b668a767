import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;

public class Pylons {
	static int R,C;
	static StringBuilder builder;
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
		BufferedWriter bw=new BufferedWriter(new OutputStreamWriter(System.out));
		int T=Integer.parseInt(br.readLine());
		for (int t = 0;t < T; t++) {
			String[] A=br.readLine().split("\\s+");
			R=Integer.parseInt(A[0]);
			C=Integer.parseInt(A[1]);
			boolean possible=false;
			for (int i = 0; i < R; i++) {
				for (int j = 0; j < C; j++) {
					boolean[][] galaxy=new boolean[R][C];
					galaxy[i][j]=true;
					StringBuilder sb=new StringBuilder();
					sb.append((i+1)+" "+(j+1)+"\n");
					possible=possible|f(i,j,galaxy,1,sb);
				}
			}
			if(possible) {
				bw.write("Case #"+(t+1)+": POSSIBLE\n");
				bw.write(c);;
			}
		}
	}
	private static boolean f(int r, int c, boolean[][] galaxy, int total, StringBuilder sb) {
		if(total==R*C) {
			System.out.println("Answer:"+sb.toString());
			return true;
		}
		boolean ans=false;
		boolean gotIn=false;
		for (int i = 0; i < R; i++) {
			for (int j = 0; j < C; j++) {
				if(check(r,c,i,j)&&!galaxy[i][j]) {
					gotIn=true;
					boolean[][] g=galaxy.clone();
					g[i][j]=true;
					StringBuilder s=new StringBuilder(sb.toString());
					s.append((i+1)+" "+(j+1)+"\n");
					ans=ans|f(i,j,g,total+1,s);
				}
			}
		}
		if(!gotIn)return false;
		return ans;
	}
	private static boolean check(int r, int c, int i, int j) {
		if(r==i || c==j)return false;
		if(r-c==i-j || r+c==i+j)return false;
		return true;
	}

}
