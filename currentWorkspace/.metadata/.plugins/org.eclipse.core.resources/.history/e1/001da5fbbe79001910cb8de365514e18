import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class scheduler {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int c=sc.nextInt();
		int[] times=new int[n];
		Queue<Integer> shoppers=new LinkedList<Integer>();
		for (int i = 0; i < c; i++) {
			shoppers.add(sc.nextInt());
		}
		int min=0;
		StringBuilder sb=new StringBuilder();
		while(!shoppers.isEmpty()) {
			int sh=shoppers.remove();
			boolean found=false;
			for (int i = 0; i < times.length; i++) {
				times[i]-=min;
				if(times[i]==0 && !found) {found=true;times[i]=sh;sb.append((i+1)+" ");}
			}
			min=Integer.MAX_VALUE;
			for (int i = 0; i < times.length; i++)if(times[i]<min)times[i]=min;
		}
		String A=sb.toString();
		A=A.substring(0,A.length()-1);
		System.out.println(A);
	}

}
